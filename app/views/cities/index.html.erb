<div class="cities">
  <h1><%= t '.title' %></h1>
  <table class="cities-table">
    <thead>
    <tr>
      <th><%= t '.city_name' %></th>
    </tr>
    </thead>
    <tbody>
    <% @cities.each do |city| %>
      <tr>
        <td><%= city.name %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>